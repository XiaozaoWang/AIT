<h2>complaints</h2>
<form method="GET" action="">
		<select name="line">
				{{#each complaints as |c|}}
				<option value={{c.line}}> {{c.line}}</option>
				{{/each}}
		</select>
		<input type="submit">	
</form>
<table>
{{#each complaints}}
<tr>
		<td>{{line}}</td>	
		<td>{{complaintText}}</td>	
		<td>{{cats}}</td>	
</tr>
{{/each}}
</table>